{
  "compilerOptions": {
    "target": "es6",
    "module": "NodeNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./app",
    "declaration": true
  },
  "include": ["app/**/*.ts"],
  "exclude": ["node_modules"]
}